{
  "dataset_config": {
    "input_dataset_name": "multiclinsum_rationale",
    "output_dataset_name": "multiclinsum_rationale_mult"
  },
  "split_config": {
    "train_ratio": 0.8,
    "valid_ratio": 0.01,
    "test_ratio": 0.19
  },
  "processing_config": {
    "drop_columns": {
      "train": ["rationale_prompt"],
      "valid": ["rationale_prompt"],
      "test": ["rationale_prompt", "rationale"]
    },
    "crop_data": {
      "enabled": true,
      "input_max_length": 2560,
      "output_max_length": 512,
      "apply_to": ["train", "valid"]
    }
  }
}
